+	.align	2
+	.globl	strcat
+	.text
+	.ent	strcat
+strcat:
+	.frame	$sp,32,$31		# vars= 0, regs= 4/0, args= 0, extra= 0
+	.mask	0x80010000,-16
+	.fmask	0x00000000,0
+	subu	$sp,$sp,32
+	sq	$16,0($sp)
+	move	$16,$4
